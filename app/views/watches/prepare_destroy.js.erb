$('.modal-body').html('Would you watch <%= @watch.title %> again?');
$('.modal-header').remove(); // don't need a header here


$('.not-watch-again-span').html("<%= escape_javascript(link_to "I would NOT watch it again",
unlike_path(likeable_id: @likeable.id, likeable_type: @likeable.class ), remote: true, method: :get , class: 'unlike'
) %>");
